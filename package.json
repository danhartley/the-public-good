{
  "name": "the-public-good",
  "version": "0.1.0",
  "private": true,
  "size-limit": [
    {
      "limit": "250 ms",
      "path": "pages/index.js"
    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "jest",
    "play": "jest play/responsible.test.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
