{
  "name": "the-public-good",
  "version": "0.1.0",
  "private": true,
  "size-limit": [
    {
      "limit": "250 ms",
      "path": "pages/index.js"
    }
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "next export",
    "test": "jest",
    "play": "jest play/responsible.test.ts",
    "plays": "jest play/generics.test.ts",
    "pdd": "jest components/pdd/*.test.ts",
    "dash": "jest components/pdd/dashboard-controls-slider.test.tsx",
    "funcs": "jest components/functions/functions.test.ts",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "size": "npm run build && size-limit",
    "cucumber": "./node_modules/.bin/cucumber-js cypress/integration/dashboard/**/*.feature --import cypress/support/step_definitions/**/*.js",
    "cypress:open": "cypress open",
    "prettier-format": "prettier --config .prettierrc 'pages/**/*.tsx' --write"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^10.2.0",
    "@tgwf/co2": "^0.15.0",
    "@types/ramda": "^0.28.1",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "next-compose-plugins": "^2.2.1",
    "ramda": "^0.28.0",
    "react": "npm:@preact/compat@0.0.4",
    "react-dom": "npm:@preact/compat@0.0.4",
    "react-query": "^3.34.17",
    "react-ssr-prepass": "npm:preact-ssr-prepass@^1.1.3",
    "sass": "^1.32.8",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^7.3.2",
    "@cucumber/pretty-formatter": "^1.0.0-alpha.2",
    "@size-limit/file": "^4.10.2",
    "@size-limit/time": "^4.10.2",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.3",
    "@types/jest": "^27.4.0",
    "@types/node": "^14.14.31",
    "@types/react": "^18.0.1",
    "chart.js": "^3.5.1",
    "chartjs-plugin-annotation": "^1.0.2",
    "chartjs-plugin-datalabels": "^2.0.0",
    "cypress": "^9.5.2",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "d3": "^7.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-functional": "^3.6.0",
    "jest": "^27.5.1",
    "jest-preset-preact": "^4.0.5",
    "next": "^11.1.2",
    "next-plugin-preact": "^3.0.4",
    "next-pwa": "^5.2.21",
    "next-secure-headers": "^2.2.0",
    "preact": "^10.5.13",
    "preact-render-to-string": "^5.1.19",
    "prettier": "2.6.2",
    "react-carbonbadge": "^1.0.1",
    "react-chartjs-2": "^3.0.5",
    "resize-observer": "^1.0.2",
    "size-limit": "^4.10.2",
    "size-plugin": "^3.0.0",
    "speed-measure-webpack-plugin": "^1.5.0",
    "ts-jest": "^27.1.2",
    "typescript": "^4.6.2",
    "venn.js": "^0.2.20",
    "webpack": "^5.61.0",
    "webpack-dashboard": "^3.3.7"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false
  }
}
